{
  "name": "jsdoc-react-templates",
  "version": "0.0.1",
  "description": "React based templating for JSDocs",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack -p --config=webpack.config.js",
    "start": "npm run webpack",
    "standardize": "standard --fix",
    "test": "standard",
    "webpack": "webpack-dev-server --inline --watch=true"
  },
  "keywords": [
    "jsdoc",
    "react"
  ],
  "author": "Ryan McNeely",
  "license": "GPLv3",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "standard": "^12.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
